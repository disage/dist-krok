Data(url:string, data:any, method:string) {\r\n    fetch(url, {\r\n      method: method, \r\n      mode: \"cors\",\r\n      headers: {\r\n        \"Content-Type\": \"application/json\"\r\n    },\r\n      body: JSON.stringify(data),\r\n    })\r\n      .then(res => res.json())\r\n      .then(json => {\r\n        this.response = json\r\n        // console.log(toJS(this.response))\r\n      })\r\n  }\r\n}\r\n","C:\\Users\\krech\\jsProjects\\cm-krok\\client\\src\\сomponents\\CourseItem\\CourseItem.tsx",[],"C:\\Users\\krech\\jsProjects\\cm-krok\\client\\src\\сomponents\\SubjectItem\\SubjectItem.js",[],"C:\\Users\\krech\\jsProjects\\cm-krok\\client\\src\\сomponents\\MaterialItem\\MaterialItem.js",[],"C:\\Users\\krech\\jsProjects\\cm-krok\\client\\src\\сomponents\\adminForm\\adminForm.js",["94","95","96","97","98","99","100"],"C:\\Users\\krech\\jsProjects\\cm-krok\\client\\src\\сomponents\\AdminForm\\AdminForm.js",["101","102"],{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":28,"column":6,"nodeType":"109","endLine":28,"endColumn":8,"suggestions":"110"},{"ruleId":"107","severity":1,"message":"111","line":17,"column":6,"nodeType":"109","endLine":17,"endColumn":8,"suggestions":"112"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"113","severity":1,"message":"114","line":1,"column":29,"nodeType":"115","messageId":"116","endLine":1,"endColumn":33},{"ruleId":"117","severity":1,"message":"118","line":5,"column":10,"nodeType":"115","messageId":"116","endLine":5,"endColumn":15},{"ruleId":"119","severity":2,"message":"120","line":5,"column":29,"nodeType":"115","endLine":5,"endColumn":37},{"ruleId":"117","severity":1,"message":"121","line":6,"column":18,"nodeType":"115","messageId":"116","endLine":6,"endColumn":27},{"ruleId":"119","severity":2,"message":"120","line":6,"column":31,"nodeType":"115","endLine":6,"endColumn":39},{"ruleId":"122","severity":2,"message":"123","line":